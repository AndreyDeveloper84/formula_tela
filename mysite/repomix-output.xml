}

@font-face {
    font-family: 'NT Somic';
    src: url('../fonts/NTSomic-Regular.woff2') format('woff2'),
        url('../fonts/NTSomic-Regular.woff') format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'NT Somic Bd';
