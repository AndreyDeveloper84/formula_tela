    }
}
html {
  scroll-behavior: smooth;
}

body {
  font-family: 'NT Somic', Arial, sans-serif;
  font-weight:normal;
  font-style: normal;
  color: #333;
  overflow-x: hidden;
  font-size:16px;
  line-height:1.2;
}

* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

a,a:hover {color:#333;}

.navbar-toggler:focus,
a:focus,
input:focus,
textarea:focus,
button:focus,
.btn:focus,
.btn.focus,
.btn:not(:disabled):not(.disabled).active,
.btn:not(:disabled):not(.disabled):active {
  text-decoration: none;
  outline: none;
  box-shadow: none;
}

a:hover {
  transition: all 0.3s ease-out 0s;
  
}

a:focus,
a:hover {
  text-decoration: none;
}

button {
  cursor: pointer;
}

i,
span,
a {
  display: inline-block;
  text-decoration: none;
}

audio,
canvas,
iframe,
img,
svg,
video {
  vertical-align: middle;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: normal;
  margin: 0px;
  margin-bottom:40px;
  text-transform:uppercase;
  font-family: 'NT Somic Bd';
}

h1 a,
h2 a,
h3 a,
h4 a,
h5 a,
h6 a {
  color: inherit;
}

.heading h1 {
  font-size: 40px;
}

h1,h2,h3,h4,h5,h6 {
  font-size: 24px;
}


ul, ol {
  margin: 0px;
  padding: 0px;
  list-style-type: none;
}

p {
  margin: 0 0 20px 0;
}

img.responsive {width:100%;}

/*===== All Button Style =====*/
.t-btn,.t-btn:hover {
  display: inline-block;
  font-family: 'NT Somic Med';
  font-weight: 500;
  user-select: none;
  padding: 0 22px;
