class Bundle(models.Model):
    name = models.CharField(max_length=120, verbose_name="–ù–∞–∑–≤–∞–Ω–∏–µ –∫–æ–º–ø–ª–µ–∫—Å–∞", null=True, blank=True)  # –Ω–∞–∑–≤–∞–Ω–∏–µ –∫–æ–º–ø–ª–µ–∫—Å–∞ (–Ω–∞–±–æ—Ä —É—Å–ª—É–≥)
    # –ø—Ä–æ—Å—Ç–∞—è –ª–æ–≥–∏–∫–∞ —Ü–µ–Ω—ã: –ª–∏–±–æ —Ñ–∏–∫—Å–∏—Ä–æ–≤–∞–Ω–Ω–∞—è, –ª–∏–±–æ —Å—É–º–º–∞ —É—Å–ª—É–≥ –º–∏–Ω—É—Å —Å–∫–∏–¥–∫–∞
    fixed_price = models.DecimalField(max_digits=8, decimal_places=2, null=True, blank=True)
    discount = models.DecimalField(max_digits=8, decimal_places=2, default=Decimal("0.00"))

    # —Å–≤—è–∑—å ¬´–º–Ω–æ–≥–∏–µ-–∫–æ-–º–Ω–æ–≥–∏–º¬ª —á–µ—Ä–µ–∑ –ø—Ä–æ–º–µ–∂—É—Ç–æ—á–Ω—É—é —Ç–∞–±–ª–∏—Ü—É,
    # –≤ –∫–æ—Ç–æ—Ä–æ–π —Ö—Ä–∞–Ω–∏—Ç—Å—è –ø–æ—Ä—è–¥–æ–∫ —É—Å–ª—É–≥ –≤–Ω—É—Ç—Ä–∏ –∫–æ–º–ø–ª–µ–∫—Å–∞
    options = models.ManyToManyField('ServiceOption', through='BundleItem', related_name='bundles')

    is_active = models.BooleanField(default=True, verbose_name="–ê–∫—Ç–∏–≤–µ–Ω")
    is_popular = models.BooleanField(default=False, verbose_name="–ü–æ–ø—É–ª—è—Ä–Ω—ã–π")
    
    class Meta:
        verbose_name = "–ö–æ–º–ø–ª–µ–∫—Å (–Ω–∞–±–æ—Ä —É—Å–ª—É–≥)"
        verbose_name_plural = "–ö–æ–º–ø–ª–µ–∫—Å—ã (–Ω–∞–±–æ—Ä—ã —É—Å–ª—É–≥)"
        indexes = [
            models.Index(fields=["is_active", "is_popular"]),
        ]

    def __str__(self):
        return self.name

    # –ø–æ—Å—á–∏—Ç–∞–µ–º –∏—Ç–æ–≥–æ–≤—É—é —Ü–µ–Ω—É: –ª–∏–±–æ fixed, –ª–∏–±–æ —Å—É–º–º–∞ - —Å–∫–∏–¥–∫–∞
    def total_price(self) -> Decimal:
        if self.fixed_price is not None:
            return self.fixed_price
        # —Å—É–º–º–∏—Ä—É–µ–º —Ü–µ–Ω—É –≤–∞—Ä–∏–∞–Ω—Ç–æ–≤ —Å —É—á—ë—Ç–æ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞
        items = self.items.select_related("option").all()
        total = Decimal("0.00")
        for it in items:
            if it.option and it.option.price is not None:
                total += Decimal(it.option.price) * it.quantity
        return max(Decimal("0.00"), total - self.discount)

    # –ø—Ä–æ—Å—Ç–∞—è –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å: —Å—É–º–º–∞ –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–µ–π –≤—Å–µ—Ö —É—Å–ª—É–≥
    # (–±–µ–∑ –ø–∞—Ä–∞–ª–ª–µ–ª—å–Ω–æ—Å—Ç–∏ –∏ –ø–∞—É–∑ ‚Äî —á—Ç–æ–±—ã –±—ã–ª–æ –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ –ø–æ–Ω—è—Ç–Ω–æ)
    def total_duration_min(self) -> int:
        # –ø—Ä–æ—Å—Ç–∞—è —Å—É–º–º–∞ –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–µ–π –≤–∞—Ä–∏–∞–Ω—Ç–æ–≤ —Å —É—á—ë—Ç–æ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞
        items = self.items.select_related("option").all()
        return sum((it.option.duration_min if it.option else 0) * it.quantity for it in items)

