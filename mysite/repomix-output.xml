    short = models.CharField(max_length=100, blank=True, null=True, verbose_name="–ö—Ä–∞—Ç–∫–æ–µ –Ω–∞–∑–≤–∞–Ω–∏–µ") 
    category = models.ForeignKey('ServiceCategory', on_delete=models.PROTECT, related_name='services', null=True, blank=True, verbose_name="–ö–∞—Ç–µ–≥–æ—Ä–∏—è")
    
    duration = models.PositiveIntegerField(default=60, verbose_name="–î–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å (–º–∏–Ω)", null=True, blank=True)  # —É—Å—Ç–∞—Ä–µ–≤—à–µ–µ –ø–æ–ª–µ
    price = models.DecimalField(max_digits=8, decimal_places=2, verbose_name="–¶–µ–Ω–∞", null=True, blank=True)
    duration_min = models.PositiveIntegerField(default=60, verbose_name="–î–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å (–º–∏–Ω)", null=True, blank=True)
    price_from = models.DecimalField(max_digits=8, decimal_places=2, verbose_name="–¶–µ–Ω–∞ –æ—Ç", null=True, blank=True)
    image = models.ImageField(
        upload_to="services/",
        blank=True,
        null=True,
        verbose_name="–ò–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ —É—Å–ª—É–≥–∏",
        help_text="–†–µ–∫–æ–º–µ–Ω–¥—É–µ–º—ã–π —Ä–∞–∑–º–µ—Ä: 800x600px"
    )
    
    class Meta:
        verbose_name = "–£—Å–ª—É–≥–∞"
        verbose_name_plural = "–£—Å–ª—É–≥–∏"
        indexes = [
            models.Index(fields=["is_active", "is_popular"]),
        ]

    def __str__(self):
        return self.name
    

UNIT_CHOICES = [
    ("session", "–ø—Ä–æ—Ü–µ–¥—É—Ä—ã"),  # –ø–∞–∫–µ—Ç –ø—Ä–æ—Ü–µ–¥—É—Ä
    ("zone",    "–∑–æ–Ω—ã"),       # –ø–∞–∫–µ—Ç –∑–æ–Ω
    ("visit",   "–≤–∏–∑–∏—Ç—ã"),     # –ø–∞–∫–µ—Ç –≤–∏–∑–∏—Ç–æ–≤
]

class ServiceOption(models.Model):

    name = models.CharField(max_length=120, verbose_name="–ù–∞–∑–≤–∞–Ω–∏–µ –≤–∞—Ä–∏–∞–Ω—Ç–∞ —É—Å–ª—É–≥–∏", null=True, blank=True)

    service = models.ForeignKey(
        Service, on_delete=models.CASCADE, 
        related_name="options", 
        verbose_name="–£—Å–ª—É–≥–∞"
        )
    duration_min = models.PositiveIntegerField(verbose_name="–î–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å (–º–∏–Ω)")
    unit_type = models.CharField(
        max_length=16,
        choices=UNIT_CHOICES,
        default="session",
        verbose_name="–ï–¥–∏–Ω–∏—Ü–∞",
    )
    units = models.PositiveSmallIntegerField(
        default=1,
        verbose_name="–ö–æ–ª-–≤–æ –µ–¥–∏–Ω–∏—Ü",
        help_text="–ù–∞–ø—Ä.: 5 –ø—Ä–æ—Ü–µ–¥—É—Ä, 3 –∑–æ–Ω—ã, 2 –≤–∏–∑–∏—Ç–∞",
    )
    price = models.DecimalField(
        max_digits=8, 
        decimal_places=0, 
        verbose_name="–¶–µ–Ω–∞",
        help_text="–ü–æ–ª–Ω–∞—è —Å—Ç–æ–∏–º–æ—Å—Ç—å –ø–∞–∫–µ—Ç–∞"
        )
    is_active = models.BooleanField(default=True, verbose_name="–ê–∫—Ç–∏–≤–Ω–∞")
    order = models.PositiveIntegerField(default=0, verbose_name="–ü–æ—Ä—è–¥–æ–∫ –ø–æ–∫–∞–∑–∞")

    yclients_service_id = models.CharField(
        max_length=50,
        blank=True,
        null=True,
        verbose_name="ID —É—Å–ª—É–≥–∏ –≤ YCLIENTS"
    )

    class Meta:
        verbose_name = "–í–∞—Ä–∏–∞–Ω—Ç —É—Å–ª—É–≥–∏"
        verbose_name_plural = "–í–∞—Ä–∏–∞–Ω—Ç—ã —É—Å–ª—É–≥–∏"
        unique_together = [("service", "duration_min", "unit_type", "units")]
        ordering = ["order", "duration_min", "unit_type", "units"]
        constraints = [
            models.CheckConstraint(
                condition=Q(duration_min__gte=1), name='svcopt_duration_gte_1'
                ),
            models.CheckConstraint(
                condition=Q(units__gte=1), name="svcopt_units_gte_1"
            )
        ]
        indexes = [
            models.Index(fields=["service", "is_active", "order"]), 
        ]

    def __str__(self):
        # –ü—Ä–∏–º–µ—Ä: "VelaShape ‚Äî 60 –º–∏–Ω √ó 10 –ø—Ä–æ—Ü–µ–¥—É—Ä"
        base = f"{self.duration_min} –º–∏–Ω"
        units_label = self.get_unit_type_display()
        suffix = f" √ó {self.units} {units_label}" if self.units and self.units > 1 else ""
        return f"{self.service.name} ‚Äî {base}{suffix}"
    
    @property
    def price_per_session(self):
        return self.price / self.units if self.units else None

class ServiceCategory(models.Model):
