}

.btn-white,.btn-white:hover {background:#fff;color: #273A37;}
.btn-white2,.btn-white2:hover {background:#fff;color: #333;border: 1px solid #333;}
.btn-black,.btn-black:hover {background:#333; border: 1px solid #333;}


.theme-btn:hover, .theme-btn:focus,.theme-btn2:hover, .theme-btn2:focus {
  color: #fff;
  box-shadow: 0px 0px 30px rgba(46, 87, 217, 0.45);
}

.theme-btn.border-btn {
  padding: 14px 40px;
  background: transparent;
  border: 1.5px solid #3763EB;
  color: #6A7C92;
  font-size: 18px;
  box-shadow: none;
}



