    class Meta:
        ordering = ["order", "name"]
        verbose_name = "–ö–∞—Ç–µ–≥–æ—Ä–∏—è —É—Å–ª—É–≥"
        verbose_name_plural = "–ö–∞—Ç–µ–≥–æ—Ä–∏–∏ —É—Å–ª—É–≥"

    def __str__(self):
        return self.name
    
class FAQ(models.Model):
    question = models.CharField(max_length=255, verbose_name="–í–æ–ø—Ä–æ—Å")
    answer = models.TextField(verbose_name="–û—Ç–≤–µ—Ç")
    order = models.PositiveIntegerField(default=0, verbose_name="–ü–æ—Ä—è–¥–æ–∫")
