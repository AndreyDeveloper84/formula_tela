===========================*/
/*===== NAVBAR =====*/


.navbar-area {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 99;
  transition: all 0.3s ease-out 0s;
  padding: 16px 0;
}

.navbar-area.header-2 {
  background: #fff;
}

.sticky {
  position: fixed;
  z-index: 99;
  background: #fff;
  box-shadow: 0px 1px 10px 0px rgba(0, 0, 0, 0.1);
  padding:0;
}

.navbar {
  position: relative;
  justify-content: space-between;
  width: 100%;
  padding:16px 0;
}


@media (max-width: 576px) {
    .container, .container-sm {
        max-width:100%;
    }
}


@media (min-width: 1280px) {
	 
    .navbar-expand-lg .navbar-collapse {
        justify-content:space-between;
}
}

.navbar-brand {
  padding: 0;
}

.navbar-toggler {
  padding: 0px;
}

.navbar-toggler .toggler-icon {
  width: 18px;
  height: 2px;
  background-color: #333;
  display: block;
  margin: 3px 0;
  position: relative;
  transition: all 0.3s ease-out 0s;
}

.navbar-toggler.active .toggler-icon:nth-of-type(1) {
  transform: rotate(45deg);
  top: 4px;
  
}

.navbar-toggler.active .toggler-icon:nth-of-type(2) {
  opacity: 0;
}

.navbar-toggler.active .toggler-icon:nth-of-type(3) {
  transform: rotate(135deg);
  top: -6px;
}


@media only screen and (min-width: 768px) and (max-width: 1199px) {
  .navbar-collapse {
    position: absolute;
    top: 100%;
    left: 0;
    width: 100%;
    background-color: #fff;
    z-index: 9;
    box-shadow: 0px 15px 20px 0px rgba(0, 0, 0, 0.1);
    padding: 24px;
    max-height: 600px;
    max-width: 500px;
    border-radius: 12px;
  }
  .social {padding:0 12px;margin-top: 20px;}
  .social a {margin-right:5px;}
  .navbar {justify-content: flex-start;}
  .navbar-toggler {margin-right: 40px;}
}

@media (max-width: 767px) {
  .navbar-collapse {
         position: fixed;
        top:0;
        left: 0;
        width: 100%;
        background-color: #fff;
        z-index: 1;
        box-shadow:none;
        padding: 5px 12px;
        height: 100%;
        padding: 15px 0 30px;
        border-radius: 0;
        overflow-y: scroll;
  }
   .social {padding:0 12px;}
   .social a {margin-right:5px;}
}

.navbar-nav .nav-item {
  position: relative;
  z-index: 1;
  flex-grow: 1;
  padding:0 20px 0 0;
}

.navbar-nav .nav-item a {
  color: #333;
  transition: all 0.3s ease-out 0s;
  position: relative;
  padding: 7px 12px;
  display:block;
  text-align:center;
  border-radius:8px;
}
.home header:not(.sticky) .navbar-nav .nav-item a {
color: #fff;}

.home header:not(.sticky) .navbar-toggler .toggler-icon {
background-color: #fff;}

@media (min-width: 768px) {
.navbar-nav a:hover,.navbar-nav a.active {
  background: #9BAE9E;color:#fff;
}

.navbar-nav a.active2:hover,.navbar-nav a.active2 {
  background: #fff;color:#333;border-bottom:1px solid #333;border-radius:0;
}

.navbar-brand {margin-right:40px;}
}


.navbar-nav .nav-item:hover > .sub-menu {
  top: calc(100% + 1px);
  opacity: 1;
  visibility: visible;
}

.navbar-nav .nav-item:hover > .sub-menu .sub-menu {
  left: 100%;
  top: 0;
}

.navbar-nav .nav-item .sub-menu {
  min-width: 320px;
  background-color: #25284b;
  position: absolute;
  top: 110%;
  left: 0;
  opacity: 0;
  visibility: hidden;
  transition: all 0.3s ease-out 0s;
  padding: 0 20px;
  border-radius:10px;
  min-height:306px;
}
.navbar-nav .nav-item .sub-menu .sub-menu {min-height:306px;min-width: 280px;border-radius:0 10px 10px 0;background:#3e4164;}

.navbar-nav .nav-item .sub-menu li:first-child {padding-top:20px;}
.navbar-nav .nav-item .sub-menu li:last-child {padding-bottom:20px;}

.navbar-nav .nav-item .sub-menu.left-menu {
  left: -100%;
}

.navbar-nav .nav-item .sub-menu.collapse:not(.show) {
  display: block;
}

@media only screen and (min-width: 768px) and (max-width: 1199px), (max-width: 767px) {
  .navbar-nav .nav-item .sub-menu.collapse:not(.show) {
    display: none;
  }
}

@media only screen and (min-width: 768px) and (max-width: 1199px), (max-width: 767px) {
  .navbar-nav .nav-item .sub-menu {
    position: static;
    width: 100%;
    opacity: 1;
    visibility: visible;
    box-shadow: none;
  }
  .navbar-nav .nav-item .sub-menu::after {
    display: none;
  }
}

.navbar-nav .nav-item .sub-menu > li {
  display: block;
  margin-left: 0;
}

.navbar-nav .nav-item .sub-menu > li:last-child {
  border: none;
}

.navbar-nav .nav-item .sub-menu > li.active > a, .navbar-nav .nav-item .sub-menu > li:hover > a {
  color: #bacdf2;
}

.navbar-nav .nav-item .sub-menu > li > a {
  font-weight: 400;
  display: block;
  padding: 10px;
  color: #fff;
  text-transform:none;
  line-height:normal;
  text-align:left;
  font-family:'PT Sans';
}
.navbar-nav .nav-item .sub-menu > li > a:after {border:none;}
.navbar-nav .nav-item .sub-menu > li > a:hover {
  background:none;
  
  color:#bacdf2;
}



