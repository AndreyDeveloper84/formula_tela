from decimal import Decimal
from django import template
from django.contrib.humanize.templatetags.humanize import intcomma

register = template.Library()

# –ø—Ä–∞–≤–∏–ª—å–Ω—ã–µ —Ñ–æ—Ä–º—ã —Å–ª–æ–≤ –¥–ª—è —Ä—É—Å—Å–∫–æ–≥–æ
FORMS = {
    "session": ("–ø—Ä–æ—Ü–µ–¥—É—Ä–∞", "–ø—Ä–æ—Ü–µ–¥—É—Ä—ã", "–ø—Ä–æ—Ü–µ–¥—É—Ä"),
    "zone":    ("–∑–æ–Ω–∞", "–∑–æ–Ω—ã", "–∑–æ–Ω"),
    "visit":   ("–≤–∏–∑–∏—Ç", "–≤–∏–∑–∏—Ç–∞", "–≤–∏–∑–∏—Ç–æ–≤"),
}
DEFAULT_FORMS = ("–µ–¥–∏–Ω–∏—Ü–∞", "–µ–¥–∏–Ω–∏—Ü—ã", "–µ–¥–∏–Ω–∏—Ü")

def _plural_ru(n: int, forms):
    n = abs(int(n))
    if n % 10 == 1 and n % 100 != 11:
        return forms[0]
    if 2 <= n % 10 <= 4 and not 12 <= n % 100 <= 14:
        return forms[1]
    return forms[2]

def _rub(val):
    # –∞–∫–∫—É—Ä–∞—Ç–Ω–æ–µ —Ñ–æ—Ä–º–∞—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ ‚ÇΩ –¥–ª—è int/float/Decimal
    if val is None:
        return ""
    if isinstance(val, Decimal):
        q = val.quantize(Decimal("1")) if val == val.to_integral() else val.quantize(Decimal("1.00"))
        num = intcomma(q)
    else:
        num = intcomma(val)
    return f"{num} ‚ÇΩ"

@register.filter
def option_label(opt):
    """–°—Ç—Ä–æ–∫–∞ –≤–∏–¥–∞:
    '60 –º–∏–Ω √ó 10 –ø—Ä–æ—Ü–µ–¥—É—Ä ‚Äî 14 000 ‚ÇΩ (1 400 ‚ÇΩ/–ø—Ä–æ—Ü.)'
    –î–ª—è –µ–¥–∏–Ω–∏—Ü—ã (units=1) –ø–æ–∫–∞–∂–µ—Ç: '60 –º–∏–Ω ‚Äî 2 500 ‚ÇΩ'
    """
    if not opt:
        return ""
    forms = FORMS.get(getattr(opt, "unit_type", ""), DEFAULT_FORMS)

    base = f"{opt.duration_min} –º–∏–Ω"
    units = getattr(opt, "units", 1) or 1

    # —á–∞—Å—Ç—å –ø—Ä–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –µ–¥–∏–Ω–∏—Ü
    units_part = f" √ó {units} {_plural_ru(units, forms)}" if units > 1 else ""

    # —Ü–µ–Ω–∞ –∏—Ç–æ–≥–æ
    price_part = _rub(getattr(opt, "price", None))

    # —Ü–µ–Ω–∞ –∑–∞ –µ–¥–∏–Ω–∏—Ü—É (–µ—Å–ª–∏ –∏—Ö >1)
    per_unit_part = ""
    if units > 1 and getattr(opt, "price", None):
        per = (opt.price / units) if units else None
        per_unit_part = f" ({_rub(per)} –∑–∞ {_plural_ru(1, forms)})"

    return f"{base}{units_part} ‚Äî {price_part}{per_unit_part}"
